# Supabase Configuration Template
# Copy this file to .env and replace with your actual Supabase project credentials

# Supabase Project URL
# Get this from your Supabase project dashboard > Settings > API
EXPO_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anonymous Key (Public Key)
# Get this from your Supabase project dashboard > Settings > API
# This key is safe to use in client-side code
EXPO_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Supabase Service Role Key (Private Key - Keep this secret!)
# Get this from your Supabase project dashboard > Settings > API
# WARNING: This key has full database access - never expose in client code!
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Additional Supabase Configuration (Optional)
EXPO_PUBLIC_SUPABASE_REGION=us-east-1
EXPO_PUBLIC_SUPABASE_SCHEMA=public

# Development Settings
NODE_ENV=development
EXPO_PUBLIC_APP_ENV=development

# App Configuration
EXPO_PUBLIC_APP_NAME=MovaVendorApp
EXPO_PUBLIC_APP_VERSION=1.0.0

# Logging Configuration
EXPO_PUBLIC_LOG_LEVEL=debug
